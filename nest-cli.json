{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/devcross/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "tsc",
    "webpack": false,
    "tsConfigPath": "apps/devcross/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "devcross": {
      "type": "application",
      "root": "apps/devcross",
      "entryFile": "main",
      "sourceRoot": "apps/devcross/src",
      "compilerOptions": {
        "tsConfigPath": "apps/devcross/tsconfig.app.json"
      }
    },
    "user": {
      "type": "library",
      "root": "libs/user",
      "entryFile": "index",
      "sourceRoot": "libs/user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/user/tsconfig.lib.json"
      }
    },
    "puzzle": {
      "type": "library",
      "root": "libs/puzzle",
      "entryFile": "index",
      "sourceRoot": "libs/puzzle/src",
      "compilerOptions": {
        "tsConfigPath": "libs/puzzle/tsconfig.lib.json"
      }
    },
    "leaderboard": {
      "type": "library",
      "root": "libs/leaderboard",
      "entryFile": "index",
      "sourceRoot": "libs/leaderboard/src",
      "compilerOptions": {
        "tsConfigPath": "libs/leaderboard/tsconfig.lib.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/devcross"
}
